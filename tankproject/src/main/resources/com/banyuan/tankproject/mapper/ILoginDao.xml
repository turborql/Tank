<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.banyuan.tankproject.mapper.ILoginDao">
    <select id="findAll" resultType="com.banyuan.tankproject.domain.DO.Login_user">
        select * from login_user where login_name=#{login_name} and password=#{password};
    </select>
</mapper>
